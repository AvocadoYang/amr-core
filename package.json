    "pm2": "^5.3.0",
    "roslib": "^1.1.0",
    "rxjs": "^7.4.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.3.2",
    "tspath": "^2.6.8",
    "winston": "^3.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
